config_version=5

[application]

config/name="AideDeCamTest"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
boot_splash/image="res://branding/splash-test.png"
config/icon="uid://drn85d5dgvbr6"

[autoload]

AideDeCam="*res://addons/aide_de_cam/aidedecam.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/aide_de_cam/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
